{
  "formConfig": {
    "labelWidth": "120px",
    "layout": "flex"
  },
  "formItemConfigs": [
    {
      "prop": "taskCode",
      "label": "任务活动编码",
      "component": {
        "name": "input",
        "compProps": {
          "clearable": true
        }
      },
      "layout": {
        "span": 6
      }
    },
    {
      "prop": "taskName",
      "label": "任务活动名称",
      "component": {
        "name": "input",
        "compProps": {
          "clearable": true
        }
      },
      "layout": {
        "span": 6
      }
    },
    {
      "prop": "taskStatus",
      "label": "任务活动状态",
      "component": {
        "name": "select",
        "compProps": {
          "clearable": true
        },
        "options": [
          { "label": "草稿", "value": "DRAFT" },
          { "label": "待生效", "value": "NOT_STARTED" },
          { "label": "生效中", "value": "ENABLE_STATUS" },
          { "label": "已结束", "value": "ALREADY_END_STATUS" },
          { "label": "已停用", "value": "ALREADY_STOP_STATUS" }
        ]
      },
      "layout": {
        "span": 6
      }
    },
    {
      "prop": "timeRange",
      "label": "有效时间",
      "component": {
        "name": "datePicker",
        "compProps": {
          "type": "datetimerange"
        }
      },
      "layout": {
        "span": 6
      }
    },
    {
      "prop": "cityCode",
      "label": "城市",
      "component": {
        "name": "select",
        "compProps": {
          "clearable": true
        }
      },
      "layout": {
        "span": 6
      }
    },
    {
      "prop": "custom",
      "label": "",
      "component": {
        "name": "custom",
        "customSlot": "ops"
      },
      "customStyle": "margin-left: auto"
    }
  ]
}
